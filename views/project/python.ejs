<% if (title) { %>
  <h1><%= title %></h1>
<% } else { %>
  <script>
      window.location.href = '/';
  </script>
<% } %>

<%- messages() %> 

<% if (errors) { %>
<ul class="notice">
  <% errors.array().forEach(error => { %>
    <li><%= error.msg %></li>
  <% }) %>
</ul>
<% } %>

